# Axon Browser Worker Configuration
# Generated by Ansible - Do not edit manually

server:
  host: 0.0.0.0
  port: 8080

adspower:
  api_url: http://127.0.0.1:50325
  timeout: 30

worker:
  artifacts_dir: /data/artifacts
  max_concurrent: {{ max_concurrent | default(5) }}

{% if central_url is defined %}
central:
  url: {{ central_url }}
  api_key: {{ central_api_key }}
{% endif %}
